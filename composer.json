{
    "name": "unicodeveloper/laravel-ngsc",
    "description": "A Laravel 5 Package for providing a fluent API for states, lgas and cities in Nigeria
",
    "keywords": ["php","github", "laravel","Open Source","states", "cities", "ngsc", "nigeria", "unicodeveloper","laravel-ngsc"],
    "license": "MIT",
    "authors": [
      {
        "name": "unicodeveloper",
        "email": "prosperotemuyiwa@gmail.com"
      }
   ],
   "minimum-stability": "stable",
   "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.2.*",
    "guzzlehttp/guzzle": "~5.0"
   },
   "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
   "autoload": {
        "psr-4": {
          "Unicodeveloper\\Ngsc\\": "src/"
        }
   },
   "autoload-dev": {
        "psr-4": {
            "Unicodeveloper\\Ngsc\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
